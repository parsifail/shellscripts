-A OUTPUT -o eth2 -p tcp -m conntrack --ctstate NEW -m tcp --dport 53 -j ACCEPT
-A OUTPUT -o eth2 -p tcp -m conntrack --ctstate NEW -m tcp --dport 25 -j ACCEPT
-A OUTPUT -o eth2 -p udp -m conntrack --ctstate NEW -m udp --dport 123 -j ACCEPT
-A OUTPUT -o eth2 -p tcp -m conntrack --ctstate NEW -m multiport --dports 80,443 -j ACCEPT
